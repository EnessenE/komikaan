{
  "Kestrel": {
    "Endpoints": {
      "MyHttpEndpoint": {
        "Url": "https://*:7252"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "NS_Api_Key": "",
  "NS_Api_Url": "http://localhost:3002",
  "OTLP_ENDPOINT_URL": "http://127.0.0.1:4317/"
}
