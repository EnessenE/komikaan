{
  "Kestrel": {
    "Endpoints": {
      "MyHttpEndpoint": {
        "Url": "http://*:80"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "komikaan": "Debug"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ]
  },
  "AllowedHosts": "*",
  "NSApiUrl": "https://gateway.apiportal.ns.nl",
  "SupplierMappings": {
    "Mappings": {
      "NederlandseSpoorwegen": {
        "ICE": "InternationalTrain",
        "EST": "InternationalTrain",
        "SPR": "RegionalTrain",
        "IC": "Train",
        "ICD": "Train",
        "RE": "RegionalTrain",
        "TRAIN": "Train",
        "METRO": "Metro",
        "BUS": "Bus"
      }
    }
  },
}
